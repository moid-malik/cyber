{
  "compilerOptions": {
    "target": "ES2017",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/components/Banners/BottomBanners/Banner.jsx", "src/components/Banners/Mini-Banners/MiniBanners.jsx", "src/components/Categorize-Products/Categorize.jsx", "src/components/DiscountedProducts/DiscountedProducts.jsx", "src/components/DiscountedProducts/Card/Card.jsx", "src/components/Footer/Footer.jsx", "src/components/Header/Header.jsx", "src/components/Header/Subnav/Subnav.jsx", "src/components/VerticalBanners/VerticalBanner/Banner.jsx"],
  "exclude": ["node_modules"]
}
